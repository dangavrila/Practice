{
  "bindings": [
    {
      "authLevel": "Function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "orchestrators/{functionName}",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "starter",
      "type": "orchestrationClient",
      "direction": "in"
    }
  ]
}